{
  "name": "Hello Extensions",
  "description" : "Base Level Extension",
  "version": "1.0",
  "permissions": [
    "notifications",
    "storage",
    "*://*.google.com/"
  ],
  "browser_action": {
    "default_popup": "hello.html",
    "default_icon": "hello_extensions.png"
  },
  "background": {
    "scripts": [
    ]
  },
  "manifest_version": 2,
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens hello.html"
    }
  }
}
